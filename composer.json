{
    "name": "tooinfinity/flextra",
    "description": "Minimal Laravel Authentication with Inertia, React, Vue, Svelte, and nWidart Laravel Modules",
    "keywords": [
        "laravel",
        "inertia",
        "modules",
        "react",
        "vue",
        "svelte"
    ],
    "homepage": "https://github.com/tooinfinity/flextra",
    "license": "MIT",
    "authors": [
        {
            "name": "TouwfiQ Meghlaoui",
            "email": "touwfiqdev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2|^8.3",
        "illuminate/console": "^11.0",
        "illuminate/filesystem": "^11.0",
        "illuminate/support": "^11.0",
        "illuminate/validation": "^11.0",
        "symfony/console": "^7.0"
    },
    "require-dev": {
        "laravel/framework": "^11.0",
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.19",
        "orchestra/testbench": "^8.0|^9.0",
        "rector/rector": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "TooInfinity\\Flextra\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TooInfinity\\Flextra\\FlextraServiceProvider"
            ],
            "aliases": {
                "Facade": "TooInfinity\\Flextra\\FlextraFacade"
            }
        }
    }
}
